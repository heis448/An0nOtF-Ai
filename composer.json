{
    "name": "an0notf/ai",
    "description": "An0nOtF AI - Advanced AI Assistant System",
    "type": "project",
    "require": {
        "php": "^8.2",
        "ext-sqlite3": "*",
        "ext-curl": "*",
        "ext-session": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "chmod 666 database.sqlite || true"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        }
    }
}
